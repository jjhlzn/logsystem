<%#encoding:utf-8 %>
<!DOCTYPE html>
<html>
<head>
  <title>横店影视成日志系统</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
  <meta charset="utf-8" />
</head>

<body class="change-list">
<!-- Container -->
<div id="container">
  <!-- Header -->
  <div id="header">
    <div id="branding">
      <h1 id="site-name">Log System</h1>
    </div>
  </div>
  <!-- END Header -->

  <!-- Content -->
  <div data-role="content">
    <%= yield %>
  </div>
  <!-- END Content -->

  <div id="footer"></div>
</div>
<!-- END Container -->

</body>
</html>
